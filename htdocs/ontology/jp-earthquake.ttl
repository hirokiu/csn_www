PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX schema: <http://schema.org/>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX jpe: <https://seismic.balog.jp/ontology/jp-earthquake.ttl#>

<https://seismic.balog.jp/ontology/jp-earthquake.ttl#>
    a   owl:Ontology ;
    rdfs:label  "地震オントロジー"@ja ;
    dcterms:creator [   foaf:homepage   <https://seismic.balog.jp/> ;
                        foaf:name       "hiroki_u"@ja
                    ] ;
    dcterms:issued   "2022-10-16" .

jpe:earthquake  a   owl:Class ;
    rdfs:label  "地震現象"@ja ;
    skos:prefLabel  "地震現象"@ja ;
    rdfs:subClassOf schema:Event .

jpe:observer    a   owl:Class ;
    rdfs:label  "観測点"@ja ;
    skos:prefLabel  "観測点"@ja ;
    rdfs:subClassOf schema:Thing .

jpe:hypocenter   a   owl:Class ;
    rdfs:label  "震源"@ja ;
    skos:prefLabel  "震源"@ja ;
    skos:altLabel   "震央"@ja ;
    rdfs:subClassOf schema:Event .

jpe:seismicMotion   a   owl:Class ;
    rdfs:label  "地震動"@ja ;
    skos:prefLabel  "地震動"@ja ;
    rdfs:subClassOf schema:Event .

jpe:observedWave   a   owl:Class ;
    rdfs:label  "観測波形"@ja ;
    skos:prefLabel  "観測波形"@ja ;
    rdfs:subClassOf schema:Event .


jpe:originTime  a   rdf:Property ;
    rdfs:label  "オリジンタイム"@ja ;
    skos:prefLabel  "オリジンタイム"@ja ;
    skos:altLabel   "origin time"@en .

jpe:magnitude  a   rdf:Property ;
    rdfs:label  "マグニチュード"@ja ;
    skos:prefLabel  "マグニチュード"@ja ;
    skos:altLabel   "magnitude"@en ;
    rdfs:subPropertyOf schema:value .

jpe:depth  a   rdf:Property ;
    rdfs:label  "震源の深さ"@ja ;
    skos:prefLabel  "震源の深さ"@ja ;
    rdfs:subPropertyOf schema:value .

jpe:shindo  a   rdf:Property ;
    rdfs:label  "震度"@ja ;
    skos:prefLabel  "震度"@ja ;
    skos:altLabel   "intensity"@en ;
    rdfs:subPropertyOf schema:value .

jpe:calcShindo  a   rdf:Property ;
    rdfs:label  "計測震度"@ja ;
    skos:prefLabel  "計測震度"@ja ;
    rdf:subPropertyOf schema:value .

jpe:detarminatedWay    a   rdf:Property ;
    rdfs:label  "震源決定フラグ"@ja ;
    skos:prefLabel  "震源決定フラグ"@ja ;
    rdfs:comment    "震源決定の方法"@ja .

jpe:detarminatedWith    a   rdf:Property ;
    rdfs:label  "震源決定に利用した波形"@ja ;
    skos:prefLabel  "震源決定に利用した波形"@ja ;
    rdfs:comment    "震源決定に利用した波形"@ja .

jpe:estimatedWith    a   rdf:Property ;
    rdfs:label  "震源推定に利用した波形"@ja ;
    skos:prefLabel  "震源推定に利用した波形"@ja ;
    rdfs:comment    "震源推定に利用した波形"@ja .

jpe:hasHypocenter  a   rdf:Property ;
    rdfs:label  "震源"@ja ;
    skos:prefLabel  "震源"@ja ;
    rdfs:comment    "地震現象の震源"@ja .

jpe:isHypocenterOf   a   rdf:Property ;
    rdfs:label  "震源とする地震"@ja ;
    skos:prefLabel  "震源とする地震"@ja ;
    rdfs:comment    "これを震源とする地震現象"@ja .

jpe:hypocenterKinds   a   rdf:Property ;
    rdfs:label  "震源レコード種別"@ja ;
    skos:prefLabel  "震源レコード種別"@ja ;
    rdfs:comment    "震源レコードの種別"@ja .

jpe:mainShock   a   rdf:Property ;
    rdfs:label  "本震"@ja ;
    skos:prefLabel  "本震"@ja ;
    rdfs:comment    "地震における本震（唯一）を示す"@ja .

jpe:afterShock   a   rdf:Property ;
    rdfs:label  "予震"@ja ;
    skos:prefLabel  "予震"@ja ;
    rdfs:comment    "地震における複数の予震を示す"@ja .

jpe:foreShock   a   rdf:Property ;
    skos:prefLabel  ""@ja ;
    rdfs:comment    ""@ja .

jpe:preShock   a   rdf:Property ;
    skos:prefLabel  ""@ja ;
    rdfs:comment    ""@ja .

jpe:stationIdentifier   a   rdf:Property ;
    rdfs:subPropertyOf  schema:identifier ;
    rdfs:label  "観測点番号"@ja ;
    skos:prefLabel  "観測点番号"@ja ;
    rdfs:comment    "各観測期間が定めた観測点のID"@ja .

jpe:withTravelTimeTable  a   rdf:Property ;
    rdfs:label      "使用走時表"@ja ;
    skos:prefLabel  "使用走時表"@ja ;
    rdfs:comment    "震源を決定するために使った走時表の種類"@ja .

jpe:observedStaionNum    a   rdf:Property ;
    rdfs:label      "観測点数" ;
    rdfs:comment    "震度１以上を観測した観測点の数"@ja .

jpe:observedBy    a   rdf:Property ;
    rdfs:label      "波形を観測した観測点" ;
    rdfs:comment    "波形を観測した観測点"@ja .

jpe:observationNetwork    a   rdf:Property ;
    rdfs:label      "観測網" ;
    rdfs:comment    "観測点が所属する観測網の名称"@ja .